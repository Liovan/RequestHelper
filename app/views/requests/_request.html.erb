<tr>
  <td><%= request.student.f_name %></td>
  <td><%= request.student.l_name %></td>
  <td><%= request.student.city %></td>
  <td><%= request.feature.name %></td>
  <td>
    <%= link_to new_request_path,class:"btn btn-success" do %>
        <i class="fa fa-print" aria-hidden="true"></i>
        <span>صدور</span>
    <%end%>
    <%= link_to new_request_path,class:"btn btn-info" do %>
        <i class="fa fa-check-square-o" aria-hidden="true"></i>
        <span>تایید</span>
    <%end%>
    <%= link_to new_request_path,class:"btn btn-danger" do %>
        <i class="fa fa-times" aria-hidden="true"></i>
        <span>برگشت</span>
    <%end%>
  </td>
</tr>